{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:react-hooks/recommended"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["react", "react-hooks"],
    "rules": {
        "space-before-function-paren": "off",
        "multiline-ternary": "off"
    }
}  